{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/share/**",
        "app": {
          "appId": "creative-atlas"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
